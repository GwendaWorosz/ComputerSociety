<p id="notice"><%= notice %></p>

<h1>Listing Full Time Positions</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>Company</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @full_time_positions.each do |full_time_position| %>
      <tr>
        <td><%= full_time_position.position %></td>
        <td><%= full_time_position.company %></td>
        <td><%= full_time_position.link %></td>
        <td><%= link_to 'Show', full_time_position %></td>
        <td><%= link_to 'Edit', edit_full_time_position_path(full_time_position) %></td>
        <td><%= link_to 'Destroy', full_time_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Full time position', new_full_time_position_path %>
